{
    "name": "axioslibrary",
    "version": "2.2.1",
    "description": "A library that can handle all the type of request using axios.",
    "main": "./build/library.js",
    "scripts": {
        "start": "node ./build/index.js",
        "build": "tsc -p .",
        "dev": "nodemon ./src/index.ts",
        "test": "mocha './test/*.spec.ts' ",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write \"./**/*.{ts,js,json}\" && npm run lint:fix"
    },
    "author": "Shubham Manohar",
    "license": "ISC",
    "dependencies": {
        "@types/express": "^4.17.15",
        "@types/node": "^18.11.18",
        "abort-controller": "^3.0.0",
        "axios": "^1.2.2",
        "chai": "^4.3.7",
        "express": "^4.18.2",
        "mocha": "^10.2.0",
        "node-fetch": "^2.6.9",
        "ts-node": "^10.9.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shubham262/axioslibrary"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "eslint": "^8.34.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "prettier": "^2.8.4",
        "typescript": "^4.9.5"
    }
}
